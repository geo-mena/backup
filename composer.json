{
    "name": "geomx/database-backup",
    "description": "A robust and flexible database backup utility for MySQL, PostgreSQL, and MongoDB",
    "keywords": [
        "backup",
        "database",
        "mysql",
        "postgresql",
        "mongodb",
        "laravel",
        "cli"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/geo-mena/database-backup/issues",
        "source": "https://github.com/geo-mena/database-backup"
    },
    "authors": [
        {
            "name": "Geovanni Mena",
            "email": "geomenacontact@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "aws/aws-sdk-php": "^3.336",
        "laravel-zero/framework": "^11.36.1",
        "mongodb/mongodb": "^1.20",
        "nunomaduro/termwind": "^2.3",
        "php-school/cli-menu": "^4.3",
        "spatie/db-dumper": "^3.7",
        "symfony/process": "^7.2"
    },
    "require-dev": {
        "laravel/pint": "^1.18.3",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1"
    },
    "autoload": {
        "psr-4": {
            "Geomx\\DatabaseBackup\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geomx\\DatabaseBackup\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Geomx\\DatabaseBackup\\Providers\\DatabaseBackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "application"
    ]
}
